{
  "name": "music-routes-data",
  "version": "1.0.7",
  "description": "What musicians played on what tracks",
  "homepage": "https://github.com/Trott/music-routes-data",
  "files": [
    "/bin",
    "/data",
    "/doc",
    "/example",
    "/lib",
    "README.md"
  ],
  "scripts": {
    "test": "grunt"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Trott/music-routes-data.git"
  },
  "keywords": [
    "music"
  ],
  "author": "Rich Trott <rtrott@gmail.com> (https://github.com/Trott)",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/Trott/music-routes-data/issues"
  },
  "contributors": [
    "Trott"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "glob": "^7.1.2",
    "grunt": "^1.1.0",
    "grunt-cli": "^1.2.0",
    "grunt-exec": "~0.4.2",
    "grunt-jsonlint": "~1.0.4",
    "jasmine": "^3.4.0",
    "semantic-release": "^17.2.1",
    "semistandard": "^14.2.2"
  },
  "dependencies": {
    "minimist": "^1.2.5"
  },
  "semistandard": {
    "env": {
      "jasmine": true
    }
  },
  "release": {
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "CHANGELOG"
            }
        ],
        "@semantic-release/npm",
        [
            "@semantic-release/git",
            {
                "assets": [
                    "CHANGELOG",
                    "package.json"
                ],
                "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
            }
        ]
    ]
  }
}
